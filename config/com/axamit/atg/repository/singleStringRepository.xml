<?xml version="1.0" encoding="utf-8" standalone="no"?>

<!DOCTYPE gsa-template
        PUBLIC "-//Art Technology Group, Inc.//DTD General SQL Adapter//EN"
        "http://www.atg.com/dtds/gsa/gsa_1.0.dtd">

<gsa-template>

	<header>
		<name>SingleStringRepository</name>
		<author>Yury Kozub</author>
	</header>

	<item-descriptor name="singleString" display-name="SingleString">
		<table name="single_strings" type="primary" id-column-name="id">
			<property name="id" column-name="id" />
			<property name="msg" column-name="msg" display-name="msg" data-type="string" />
		</table>
	</item-descriptor>

</gsa-template>

